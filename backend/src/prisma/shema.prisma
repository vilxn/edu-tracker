datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Shanyrak {
  id      String  @id @default(uuid())
  name    String  @unique
  points  Int     @default(0)
  members Int     @default(0)
  color   String

  @@map("shanyraks")
}